<p id="notice"><%= notice %></p>

<h1>Competency Raitings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Rank</th>
      <th>Description</th>
      <th>Competency</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @competency_raitings.each do |competency_raiting| %>
      <tr>
        <td><%= competency_raiting.name %></td>
        <td><%= competency_raiting.rank %></td>
        <td><%= competency_raiting.description %></td>
        <td><%= competency_raiting.competency %></td>
        <td><%= link_to 'Show', competency_raiting %></td>
        <td><%= link_to 'Edit', edit_competency_raiting_path(competency_raiting) %></td>
        <td><%= link_to 'Destroy', competency_raiting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Competency Raiting', new_competency_raiting_path %>
